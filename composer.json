{
    "name": "laravel/laravel",
    "type": "project",
    "description": "EHR Epic.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-pdo": "*",
        "brianium/paratest": "^4.0",
        "brick/math": "^0.8.15",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0",
        "kevinrob/guzzle-cache-middleware": "^3.3",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "laravel/dusk": "^6.4",
        "laravel/telescope": "^3.5",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "php-parallel-lint/php-console-highlighter": "^0.5.0",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "php-webdriver/webdriver": "^1.8",
        "phpstan/phpstan": "^0.12.32",
        "phpunit/phpunit": "^8",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": "php-parallel-lint app tests",
        "phpcs": "phpcs app tests --standard=psr2 -sp --colors",
        "phpcbf": "phpcbf app tests --standard=psr2 -sp --colors",
        "phpstan": [
            "phpstan analyse -c phpstan.neon app --level max --no-progress",
            "phpstan analyse -c phpstan-tests.neon tests --level 4 --no-progress"
        ],
        "phpunit": "phpunit --verbose --colors=always",
        "test": [
            "@lint",
            "@phpstan",
            "@phpunit",
            "@phpcs"
        ]
    }
}
